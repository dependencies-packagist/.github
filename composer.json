{
    "name": "dependencies/packagist",
    "description": "The Dependencies Package Repository.",
    "keywords": [
        "dependencies",
        "packagist"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/dependencies-packagist",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "annotation/enumeration": "^1.0",
        "annotation/routing": "^2.1",
        "gmtls/openssl-crypto-kit": "^3.0",
        "standards/urn": "^2.0",
        "token/jwk": "^1.0",
        "token/jwt": "^4.0"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
